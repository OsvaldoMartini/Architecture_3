@using MongoDB.Bson
@using MongoDB.Driver
@model MongoCursor<BsonDocument>
@*@model Arquitetura.MongoDB.CaixaAzul*@

@{
    ViewBag.Title = "Index";
}

<h2>
    Caixa Azul</h2>
            <p>
                @Html.ActionLink("New Caixa Azul", "New", new { Controller = "CaixaAzul" })
            </p>
<table cellpadding="0" cellspacing="0">
    <tr>
        <td>
            CNPJ CD
        </td>
        <td>
            Numero Caixa
        </td>        
    </tr>

    @foreach (var doc in Model)
    {
        <tr>
            <td>
                @doc["cnpjCD"].AsString
            </td>
            <td>
                @doc["numeroCaixa"].AsBsonValue
            </td>
           
            <td>
                @Html.ActionLink("Edit caixaAzul", "Edit", new { id = doc["_id"].ToString() })
            </td>
            <td>
                @Html.ActionLink("Entregas", "Index", new { controller = "Entrega", id = doc["_id"].ToString() })
            </td>
        </tr>
    }
</table>

